generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model user {
  id String @unique @id @default(uuid())
  name String
  avatar String
  email String?
  accountNumber Int 
  isPremium Boolean @default(false)
}